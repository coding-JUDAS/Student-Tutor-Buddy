<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane expanded="true" prefHeight="320.0" prefWidth="490.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TutorLandingPage.session.SessionEditController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <content>
      <GridPane prefHeight="214.0" prefWidth="473.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="203.0" minWidth="10.0" prefWidth="123.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="293.0" minWidth="10.0" prefWidth="293.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label prefHeight="17.0" prefWidth="111.0" text="Student Number" />
            <Label text="Module Code" GridPane.rowIndex="1" />
            <Label text="Session date" GridPane.rowIndex="2" />
            <Label text="Session Time" GridPane.rowIndex="3" />
            <Label fx:id="lblNumStudents" text="Number of Students" GridPane.rowIndex="4" />
            <Label fx:id="lblFee" text="Fee" GridPane.rowIndex="5" />
            <TextField fx:id="txtFieldStudentNo" GridPane.columnIndex="1" />
            <ChoiceBox fx:id="choiceBoxModuleCode" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <DatePicker fx:id="datePickerSessionDate" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Spinner fx:id="spinnerSessionTime" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="txtFieldNumStudents" prefHeight="25.0" prefWidth="209.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="txtFieldFee" GridPane.columnIndex="1" GridPane.rowIndex="5" />
         </children>
      </GridPane>
   </content>
   <header>
      <HBox prefHeight="44.0" prefWidth="473.0" spacing="20.0">
         <children>
            <RadioButton fx:id="radioBtnGroupSession" mnemonicParsing="false" text="Group Session" />
            <RadioButton fx:id="radioBtnIndividualSession" mnemonicParsing="false" text="Individual Session" />
         </children>
      </HBox>
   </header>
   <buttonTypes>
      <ButtonType fx:constant="APPLY" />
      <ButtonType fx:constant="CLOSE" />
   </buttonTypes>
</DialogPane>
